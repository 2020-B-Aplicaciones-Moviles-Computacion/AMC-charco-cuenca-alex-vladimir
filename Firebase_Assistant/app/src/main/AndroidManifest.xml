        <activity android:name=".COrdenes" />
        <activity android:name=".CProduto" />
        <activity android:name=".CRestaurante" />
        <activity android:name=".BFirestore" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>